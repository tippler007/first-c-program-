#include <stdio.h>
int main()
{
    char ch;
    printf("Enter the letter:");
    scanf("%c",&ch);
    if (ch=='a')
    {
        printf("it's vowel");
    }
    else if ( ch=='e')
        printf("it's  vowel");
    else if ( ch=='i')
        printf("it's  vowel");
    else if ( ch=='o')
        printf("it's  vowel");
    else if ( ch=='u')
        printf("it's  vowel");
    else
        printf("it's a consonent\n");
    return 0;
    

        
        
}
